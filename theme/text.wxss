/*
@exlarge-font-size: @font-size-base + 10 !important; // 44rpx
@xlarge-font-size: @font-size-base + 8 !important; //40rpx
@large-font-size: @font-size-base + 2 !important;  //34rpx
@font-size-base: 32rpx !important;
@small-font-size: @font-size-base - 4 !important;  //28rpx
@xsmall-font-size: @font-size-base - 8 !important; //24rpx
*/
.fonts {
  font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, 'Helvetica Neue', Arial, 'Noto Sans', sans-serif, 'Apple Color Emoji', 'Segoe UI Emoji', 'Segoe UI Symbol', 'Noto Color Emoji' !important;
}
.title {
  color: #ffffff !important;
  font-weight: 500 !important;
  font-size: 36rpx !important;
}
.list {
  color: rgba(255, 255, 255, 0.85) !important;
  font-size: 34rpx !important;
}
.subtitle {
  color: #ffffff !important;
  font-size: 30rpx !important;
}
.number {
  font-variant-numeric: oldstyle-nums !important;
}
